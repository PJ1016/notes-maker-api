CREATE TABLE playerStats (
    id INT AUTO_INCREMENT PRIMARY KEY,
    player VARCHAR(50),
    Team VARCHAR(50),
    runs INT,
    highestScore INT
);
INSERT INTO playerStats (player, team, runs, highestScore) VALUES
