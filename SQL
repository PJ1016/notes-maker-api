CREATE TABLE player_info(
id INT AUTO_INCREMENT PRIMARY KEY,
playerName  VARCHAR(50),
gender  VARCHAR(50),
role  VARCHAR(50),
battingStyle  VARCHAR(50),
bowlingStyle  VARCHAR(50),
nationality VARCHAR(50),
dateOfBirth VARCHAR(50),
playerDescription VARCHAR(50))
